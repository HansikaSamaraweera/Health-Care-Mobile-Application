<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.healthcare">

    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/d12"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/d11"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".addPatient">
        </activity>
        <activity android:name=".EditNotesActivity"/>
        <activity android:name=".CalenderActivity"/>
        <activity android:name=".NotesActivity"/>
        <activity android:name=".PatientActivity"/>
        <activity android:name=".MedicineActivity"/>
        <activity
                android:name=".navActivity"
                android:label="@string/title_activity_nav"
                android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".LoginActivity"/>
        <activity
                android:name=".FullscreenActivity"
                android:configChanges="orientation|keyboardHidden|screenSize"
                android:label="@string/app_name"
                android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>